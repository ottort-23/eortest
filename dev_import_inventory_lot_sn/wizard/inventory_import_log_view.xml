<odoo>
    <record id="view_dev_inventory_import_log_form" model="ir.ui.view">
        <field name="name">Inventory Import Logs</field>
        <field name="model">inventory.import.log</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <field name="name" readonly="1"/>
                </group>
                <footer>
                    <button string="Cancel" class="btn-default" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>

    <record id="action_dev_inventory_import_log" model="ir.actions.act_window">
        <field name="name">Inventory Import Logs</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">inventory.import.log</field>
        <field name="view_mode">form</field>
        <field name="target">new</field>
    </record>
</odoo>
